<div class="modal-dialog" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title" id="exampleModalLabel">Cadastrar Task</h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
	    <div>
        <p class="teste"></p>
        <%= simple_form_for Card.new, url: create_card_path(), wrapper: :inline_form  do |form| %>
          <p>Criar nova Task: </p>
          <p><%= form.input_field :title %> </p>
          <p><%= form.input_field :description %> </p>
          <p><%= form.input_field :dateFinish %> </p>
          <p><%= form.submit "Salvar" %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>